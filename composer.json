    "require-dev": {
        "barryvdh/laravel-ide-helper": "^3.5",
        "beyondcode/laravel-dump-server": "^2.0",
        "captainhook/captainhook": "^5.19",
        "captainhook/plugin-composer": "^5.2",
        "driftingly/rector-laravel": "^0.29",
        "ergebnis/composer-normalize": "^2.38",
        "friendsofphp/php-cs-fixer": "^3.69",
        "jubeki/laravel-code-style": "^2.0",
        "larastan/larastan": "^3.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "php-coveralls/php-coveralls": "^2.7",
        "phpunit/phpunit": "^11.5.3",
        "rector/rector": "^0.18.13",
        "laravel/pail": "^1.2.2"
    },
    "suggest": {
        "ext-gd": "to use GD library based image processing.",
        "ext-imagick": "to use Imagick based image processing.",
        "ext-xdebug": "For developement work"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/DariusIII/Net_NNTP"
        },
        {
            "type": "vcs",
            "url": "https://github.com/DariusIII/html"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Blacklight\\": "Blacklight/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Extensions/helper/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "captainhook/plugin-composer": true,
            "treeware/plant": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        },
        "discard-changes": true,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "process-timeout": 3600,
        "sort-packages": true
    },
    "scripts": {
        "post-update-cmd": [
            "@php artisan ide-helper:generate --ansi",
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "check-style": "php-cs-fixer fix --dry-run --diff",
        "fix-style": "pint",
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    }
}
